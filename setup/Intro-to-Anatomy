// This is a way to 'comment out' one line of code that will be ignored by the compiler.


// Preprocessor

#include <EEPROM.h>;

const int blue = 16;
const int red = 17;

void setup() { // setup is run once at the beginning
 pinMode(blue, OUTPUT);
 pinMode(red, OUTPUT);
} // make sure to close all actions

void loop() {
  digitalWrite(blue, HIGH);
  delay(500);
  digitalWrite(blue, LOW);
  delay(500);
}
